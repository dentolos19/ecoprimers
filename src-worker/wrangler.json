{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ecoprimers",
  "main": "index.ts",
  "compatibility_date": "2026-01-01",
  "compatibility_flags": ["nodejs_compat"],
  "keep_vars": true,
  "observability": {
    "logs": {
      "enabled": true
    },
    "traces": {
      "enabled": true
    }
  },
  "routes": [
    {
      "pattern": "ecoprimers.dennise.me",
      "custom_domain": true
    }
  ],
  "containers": [
    {
      "class_name": "Server",
      "image": "../Dockerfile",
      "max_instances": 1,
      "instance_type": "basic"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "SERVER",
        "class_name": "Server"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Server"]
    }
  ]
}
